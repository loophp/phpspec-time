{
    "name": "loophp/phpspec-time",
    "type": "library",
    "description": "A PHPSpec extension providing matchers for measuring time in tests.",
    "keywords": [
        "phpspec-extension"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pol Dellaiera",
            "email": "pol.dellaiera@protonmail.com"
        }
    ],
    "require": {
        "php": ">= 7.3",
        "loophp/nanobench": "^1"
    },
    "require-dev": {
        "drupol/php-conventions": "^2.0.3 || ^3.0.0",
        "friends-of-phpspec/phpspec-code-coverage": "^5",
        "infection/infection": "^0.18.2",
        "infection/phpspec-adapter": "^0.1.1",
        "phpspec/phpspec": "^7",
        "phpstan/phpstan-strict-rules": "^0.12",
        "vimeo/psalm": "^4"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "loophp\\phpspectime\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\loophp\\phpspectime\\": "./spec/loophp/phpspectime/"
        }
    }
}
